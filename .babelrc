{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": "last 2 versions"
            }
        ]
    ],
    "plugins": [
        "@vue/babel-plugin-transform-vue-jsx"
    ],
    "env": {
        "utils": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "loose": true,
                        "modules": "commonjs",
                        "targets": {
                            "browsers": [
                                "> 1%",
                                "last 2 versions",
                                "not ie <= 8"
                            ]
                        }
                    }
                ]
            ]
        }
    }
}
