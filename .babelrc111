{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "commonjs",
                "targets": {
                    "ie": "11"
                }
            }
        ]
    ],
    "plugins": [
        "@vue/babel-plugin-transform-vue-jsx"
    ],
    "env": {
        "utils": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "loose": true,
                        "modules": "commonjs",
                        "targets": {
                            "chrome": "58",
                            "ie": "11"
                        }
                    }
                ]
            ]
        }
    }
}
